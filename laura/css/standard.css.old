
	body
	{
		color: #000066;
		background-color: white;
		font-family: serif;
		margin: 0px;
		padding: 0;
		border: 0
	}

	div
	{
		padding: 0 0 0 0;
		margin: 0 0 0 0
	}

	#top-bar
	{
		width: 100%;
		text-align: right;
		background: #CC0000;
		padding-bottom: 1px;

		color: #FFCCCC;
		text-decoration: none;
		font-weight: bold;
		font-family: sans-serif;
		font-size: 0.8em;

		border-bottom-style: solid;
		border-bottom-width: 2px;
		border-bottom-color: #000099
	}

	#top-bar A
	{
		color: #FFCCCC;
		text-decoration: none;
		font-weight: bold;
		font-family: sans-serif
	}

	#header
	{
		background: #FFCCCC;
		text-align: left;
		padding: 0.2em;
		margin: 0;
	}

	#header h1
	{
		font-size: 2em;
		font-family: sans-serif;
		font-weight: bold;
		color:#CC0000;
		padding: 0;
		margin: 0
	}


	#main-container
	{
		padding: 1em 1em 1em 1em;
		//padding: 0;
	}

		#menu
		{
			border-style: solid;
			border-width: 1px;
			border-color: #FFFFFF;
			height: 3.5em
		}

			#menu-item
			{
				float: left;
				width: 15%;
				padding: 0.5em 0em 0.5em 0em;
				margin: 0;

				background: #3366FF;

				color: white;
				font-family: sans-serif;
				font-size: 0.9em;
				font-weight: bold;
				text-align: center;

				border-style: solid;
				border-width: 2px 0 2px 0;
				border-color: #000099
			}

				#menu-item a
				{
					color: #FFFFFF;
					font-family: sans-serif;
					font-weight: bold
				}

				#menu-item.current
				{
					background: #000099;
					font-weight: bold;

					border-color: #3366FF
				}

		#contents
		{
			margin: 0em 0em 0em 0em;
			padding: 1em 1.5em 1em 1.5em;
			background: #FFFFCC;

			color: #000066;

			border-style: solid;
			border-width: 2px;
			border-color: #000099;

			text-align: justify
		}

			#contents h2
			{
				font-family: sans-serif;
				text-align: center;
				font-size: 2em;
				font-weight: bold;
				color: #CC0000;
				margin: 0px;
				margin-left: 0;
				margin-top: 0em;
				margin-bottom: 1em;
				border: 0px;
				padding: 0px
			}

			#contents h3
			{
				font-family: sans-serif;
				text-align: left;
				font-size: 1.5em;
				font-weight: bold;
				color: #000066;
				margin: 0px;
				margin-left: 0;
				margin-top: 1.3em;
				margin-bottom: 0em;
				border: 0px;
				padding: 0px
			}

			#contents h4
			{
				font-family: sans-serif;
				text-align: left;
				font-size: 1.1em;
				font-weight: bold;
				color: #000066;
				margin: 0px;
				margin-left: 0;
				margin-top: 1em;
				margin-bottom: 0em;
				border: 0px;
				padding: 0px
			}

			#contents p
			{
				border: 0px;
				padding: 0px;
				margin: 0 0 0 0
			}

			#contents a
			{
				color: #0000FF;
				border: 0px;
				padding: 0px;
			}


	h1
	{
		font-family: sans-serif;
		text-align: center;
		font-size: 175%;
		margin: 1em
	}

	h2.nomargintop
	{
		margin-top: 0px
	}

	code
	{
		background-color: #bfdfdf
	}

	pre
	{
		background-color: #bfdfdf;
	}


	#footer
	{
		text-align: left;
		background: #CC0000;
		padding-bottom: 1px;

		color: #FFCCCC;
		text-decoration: none;
		font-weight: bold;
		font-family: sans-serif;
		font-size: 0.8em;

		border-top-style: solid;
		border-top-width: 2px;
		border-top-color: #000099
	}

		#footer A
		{
			color: #0000FF;
			font-weight: bold;
			font-family: sans-serif
		}

	table
	{
		empty-cells: show;
		padding: 0
	}

	table.main
	{
		border: 1px;
		border-color: #000000;
		border-collapse: collapse;
		border-style: solid;
		
		empty-cells: show;
		align: center		
	}

	table.main TD
	{
		border: 1px;
		border-style: solid;
		padding: 0 2px 0 2px;
		align: center;		
	}

	table.main TH
	{
		border: 1px;
		border-style: solid;
	}

	table.macros
	{
		//table-layout: fixed;
		border: 0;
		border-spacing: 2px;
		
		empty-cells: show;
		margin: 0;
		align: center;		
	}

		table.macros TD
		{
			background: lightblue;
			empty-cells: show;
			border: 0px;
			border-style: none;
			align: center;		
		}

